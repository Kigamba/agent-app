<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.stavros.demo.android">

    <application
        android:name=".AgentApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".form.TenantRegistration" />
        <activity android:name=".form.RegisterLandlordUnits" />
        <activity
            android:name=".form.LandlordRegistration"
            android:label="@string/landlord_registration" />
        <activity android:name=".form.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".form.LandlordListActivity"/>
    </application>

</manifest>